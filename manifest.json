{
  "manifest_version": 2,
  "name": "Scrapnote",
  "version": "0.1",

  "browser_action": {
    "default_icon": "icon.png"
  },

  "chrome_url_overrides" : {
    "newtab": "index.html"
  },

  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-2.1.3.min.js", "content.js"]
    }
  ]
}
